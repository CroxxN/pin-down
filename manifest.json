{
    "name": "Pin Down",
    "description": "Pin a Website",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "variables.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/Pin(16x16).png",
            "32": "/images/Pin(32x32).png",
            "48": "/images/Pin(48x48).png",
            "128": "/images/Pin(128x128).png"
          }
        },
    "icons": {
        "16": "/images/Pin(16x16).png",
        "32": "/images/Pin(32x32).png",
        "48": "/images/Pin(48x48).png",
        "128": "/images/Pin(128x128).png"
    },
    "web_accessible_resources": [
        {
          "resources": [ "*.png" ],
          "matches": [ "<all_urls>" ]
        }
      ]
  }